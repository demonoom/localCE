{
  "name": "localClassAssistant",
  "version": "1.0.0",
  "description": "本地课堂",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder --win --ia32"
  },
  "build": {
    "appId": "com.excoord-localClass.app",
    "copyright": "Open Totally",
    "compression": "normal",
    "productName": "小蚂蚁同步课堂",
    "nsis": {
      "oneClick": true,
      "perMachine": true,
      "runAfterFinish": true
    },
    "win": {
      "icon": "./images/logoo.png"
    },
    "publish": [
      {
        "provider": "generic",
        "url": "http://localhost:5000/winRelease/"
      }
    ]
  },
  "keywords": [],
  "author": "noom",
  "license": "ISC",
  "devDependencies": {
    "electron": "^3.0.10",
    "electron-builder": "^20.31.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.6.1",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-html": "^4.0.6",
    "eslint-plugin-import": "^2.14.0"
  },
  "dependencies": {
    "electron-updater": "^4.0.0",
    "hashmap": "^2.3.0",
    "ws": "^6.1.0",
    "app-root-path": "^2.1.0",
    "fs-extra": "^7.0.1"
  }
}
